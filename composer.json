{
    "name": "formfeed-uk/nova-dependable-panel",
    "description": "A Laravel Nova Field acting as a Panel for grouped dependsOn functionality",
    "keywords": [
        "laravel",
        "nova",
        "panel",
        "dependsOn"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "laravel/nova": "^4.0",
        "nova-kit/nova-packages-tool":"^1.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Formfeed\\DependablePanel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Formfeed\\DependablePanel\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
